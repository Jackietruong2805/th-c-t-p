{
  "name": "piep-mark",
  "version": "0.0.22",
  "private": true,
  "scripts": {
    "local": "concurrently \"npm run serve\" \"npm run watch\" --names \"BE,FE\" -c \"blue.bold,green.bold\"",
    "serve": "php artisan serve --port 3000 --quiet",
    "build": "rm -rf ./public/build && vue-tsc --noEmit && node --max_old_space_size=16384 ./node_modules/vite/bin/vite.js build",
    "watch": "vue-tsc --noEmit && node --max_old_space_size=16384 ./node_modules/vite/bin/vite.js build --watch --mode=development",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "fontawesome": "^5.6.3",
    "ky": "^0.30.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.13",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.27",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "concurrently": "^7.2.0",
    "cypress": "^9.5.4",
    "dotenv": "^16.0.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "mitt": "^3.0.0",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.52.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.6.2",
    "unplugin-auto-import": "^0.7.2",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.34.7"
  }
}
